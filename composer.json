    "doctrine/common": "2.7.2",
    "monolog/monolog": "1.22.1"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.6",
    "phpunit/phpunit": "~5",
    "phpmetrics/phpmetrics": "^2.0"
  }
